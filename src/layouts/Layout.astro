---
import 'san-webkit-next/app.css'
import Intercom from '../components/Intercom.svelte'
import Tracking from '../components/Tracking.svelte'
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" href="/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
    <meta name="generator" content={Astro.generator} />
    <title>Crypto Research, Data, Tools - Explore Behavioral Analytics</title>
  </head>
  <body>
    <Intercom client:load>
      <Tracking client:load>
        <div data-vaul-drawer-wrapper>
          <slot />
        </div>
      </Tracking>
    </Intercom>
  </body>
</html>
